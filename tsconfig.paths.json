{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@infraConfig/*": [
        "config/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@database/*": [
        "src/database/*"
      ],
      "@modules/*": [
        "src/modules/*"
      ],
      "@schemas/*": [
        "src/schemas/*"
      ],
      "@service/*": [
        "src/service/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@models/*": [
        "src/models/*"
      ]
    }
  }
}